{
  "name": "saleor-app-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm generate && NODE_OPTIONS='--inspect' next dev",
    "build": "pnpm generate && next build",
    "start": "next start",
    "fetch-schema": "curl https://raw.githubusercontent.com/saleor/saleor/${npm_package_saleor_schemaVersion}/saleor/graphql/schema.graphql > graphql/schema.graphql",
    "generate": "pnpm generate:graphql && pnpm generate:schema",
    "generate:graphql": "graphql-codegen",
    "generate:schema": "pnpm ts-node-esm src/schemas/compile.mts",
    "lint": "next lint --fix --dir src",
    "lint:ci": "next lint --dir src",
    "test": "POLLY_MODE=replay vitest",
    "test:record": "POLLY_MODE=record_missing vitest",
    "test:ci": "CI=true vitest --coverage",
    "ts-node-esm": "node --loader ts-node/esm --experimental-specifier-resolution=node",
    "prepare": "husky install"
  },
  "saleor": {
    "schemaVersion": "3.10"
  },
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@saleor/app-sdk": "0.28.0",
    "@saleor/macaw-ui": "0.7.2",
    "@sentry/nextjs": "7.36.0",
    "@urql/exchange-auth": "1.0.0",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.28.4",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "jsdom": "21.1.0",
    "lodash": "4.17.21",
    "modern-errors": "5.5.0",
    "next": "13.1.6",
    "omit-deep-lodash": "1.1.7",
    "pino": "8.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "urql": "3.0.3",
    "vite": "4.1.1",
    "vitest": "0.28.4",
    "yup": "1.0.0-beta.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/typed-document-node": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-operations": "3.0.0",
    "@graphql-codegen/typescript-urql": "3.7.3",
    "@graphql-codegen/urql-introspection": "2.2.1",
    "@graphql-typed-document-node/core": "3.1.1",
    "@pollyjs/adapter-fetch": "6.0.5",
    "@pollyjs/adapter-node-http": "6.0.5",
    "@pollyjs/core": "6.0.5",
    "@pollyjs/persister-fs": "6.0.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/lodash": "4.14.191",
    "@types/node": "18.13.0",
    "@types/omit-deep-lodash": "1.1.1",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/setup-polly-jest": "0.5.2",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-require-form-method": "1.0.2",
    "eslint-plugin-testing-library": "5.10.0",
    "husky": "8.0.3",
    "json-schema-to-typescript": "11.0.3",
    "lint-staged": "13.1.1",
    "next-test-api-route-handler": "3.1.8",
    "prettier": "2.8.3",
    "setup-polly-jest": "0.11.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "vite-tsconfig-paths": "4.0.5"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=18.0.0",
    "pnpm": ">=7.7.1"
  },
  "packageManager": "pnpm@7.11.0",
  "pnpm": {
    "patchedDependencies": {
      "@types/testing-library__jest-dom@5.14.5": "patches/@types__testing-library__jest-dom@5.14.5.patch"
    }
  }
}
