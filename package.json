{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "saleor-app-payment-template",
  "description": "Saleor App Payment Template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm generate && NODE_OPTIONS='--inspect' next dev",
    "build": "pnpm generate && next build",
    "start": "next start",
    "fetch-schema": "curl https://raw.githubusercontent.com/saleor/saleor/${npm_package_saleor_schemaVersion}/saleor/graphql/schema.graphql > graphql/schema.graphql",
    "generate": "pnpm generate:graphql && pnpm generate:schema",
    "generate:graphql": "graphql-codegen",
    "generate:schema": "pnpm ts-node-esm src/schemas/compile.mts",
    "lint": "next lint --fix --dir src",
    "lint:ci": "next lint --dir src",
    "test": "vitest",
    "test:record": "POLLY_MODE=record_missing vitest",
    "test:rerecord": "POLLY_MODE=record vitest",
    "test:ci": "CI=true vitest --coverage --reporter=json --outputFile=coverage/report.json && ./fix-coverage-report.js",
    "ts-node-esm": "node --loader ts-node/esm --experimental-specifier-resolution=node",
    "prepare": "husky install"
  },
  "saleor": {
    "schemaVersion": "support-for-payment-apps"
  },
  "dependencies": {
    "@hookform/resolvers": "3.0.0",
    "@saleor/app-sdk": "0.37.2",
    "@saleor/macaw-ui": "0.8.0-pre.68",
    "@sentry/nextjs": "7.47.0",
    "@urql/exchange-auth": "1.0.0",
    "@vanilla-extract/css": "1.11.0",
    "@vanilla-extract/next-plugin": "2.1.1",
    "@vanilla-extract/recipes": "0.4.0",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.30.0",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "bluebird": "3.7.2",
    "classnames": "2.3.2",
    "eslint-plugin-node": "11.1.0",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "jose": "4.13.1",
    "jsdom": "21.1.1",
    "lodash-es": "4.17.21",
    "modern-errors": "5.5.4",
    "modern-errors-http": "2.3.0",
    "modern-errors-serialize": "3.0.0",
    "next": "13.3.0",
    "omit-deep-lodash": "1.1.7",
    "pino": "8.11.0",
    "pino-pretty": "10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "url-join": "5.0.0",
    "urql": "3.0.4",
    "uuidv7": "0.4.2",
    "vite": "4.2.1",
    "vitest": "0.30.0",
    "webpack": "5.78.0",
    "yup": "1.0.2",
    "zod": "3.21.4",
    "zustand": "4.3.7"
  },
  "devDependencies": {
    "@graphql-codegen/add": "4.0.1",
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typed-document-node": "4.0.0",
    "@graphql-codegen/typescript": "3.0.3",
    "@graphql-codegen/typescript-operations": "3.0.3",
    "@graphql-codegen/typescript-urql": "3.7.3",
    "@graphql-codegen/urql-introspection": "2.2.1",
    "@graphql-typed-document-node/core": "3.2.0",
    "@next/env": "13.3.0",
    "@pollyjs/adapter-fetch": "6.0.5",
    "@pollyjs/adapter-node-http": "6.0.5",
    "@pollyjs/core": "6.0.5",
    "@pollyjs/persister-fs": "6.0.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/bluebird": "3.5.38",
    "@types/lodash-es": "4.17.7",
    "@types/node": "18.15.11",
    "@types/omit-deep-lodash": "1.1.1",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "@types/setup-polly-jest": "0.5.2",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "@vanilla-extract/vite-plugin": "3.8.0",
    "dependency-cruiser": "12.11.1",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-require-form-method": "1.0.2",
    "eslint-plugin-testing-library": "5.10.2",
    "eslint-plugin-vitest": "0.0.57",
    "husky": "8.0.3",
    "json-schema-to-typescript": "12.0.0",
    "lint-staged": "13.2.1",
    "next-test-api-route-handler": "3.1.8",
    "prettier": "2.8.7",
    "setup-polly-jest": "0.11.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vite-tsconfig-paths": "4.1.0"
  },
  "engines": {
    "npm": ">=8.0.0 <10.0.0",
    "node": ">=18.0.0 <19.0.0",
    "pnpm": ">=8.0.0 <9.0.0"
  },
  "packageManager": "pnpm@8.0.0",
  "pnpm": {
    "patchedDependencies": {
      "@types/testing-library__jest-dom@5.14.5": "patches/@types__testing-library__jest-dom@5.14.5.patch"
    },
    "overrides": {
      "@urql/exchange-auth>@urql/core": "3.2.2"
    }
  }
}
